{
  "author": "Shubham",
  "dark": false,
  "name": "Vitesse Light",
  "editorScheme": "/themes/vitesseLight.xml",
  "ui": {
    "*": {
      "background": "#F1F0E9",
      "foreground": "#606060",
      "highlight": "#e7e7e8ff",
      "selectionBackground": "#ccd7daff",
      "selectionInactiveBackground": "#ccd7daff",
      "selectionForeground": "#546e7aff",
      "button": "#f3f4f5ff",
      "secondaryBackground": "#ffffffff",
      "disabled": "#d2d4d5ff",
      "contrast": "#eeeeeeff",
      "tableSelected": "#e7e7e8ff",
      "secondBorder": "#d3e1e8ff",
      "treeSelection": "#80cbc440",
      "notifications": "#eae8e8ff",
      "accent": "#00bcd4ff",
      "excluded": "#d3e1e8ff"
    },
    "Islands": 1,
    "Island": {
      "arc": 20,
      "borderWidth": 4,
      "borderColor": "#dadadb",
      "inactiveAlpha": 0.44,
      "inactiveAlphaInStatusBar": {
        "os.mac": 0.2,
        "os.windows": 0.2,
        "os.linux": 0.15
      }
    },
    "MainWindow.background": "#dadadb",
    "DefaultTabs": {
      "background": "#F1F0E9",
      "borderColor": "#F1F0E9"
    },
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "#F1F0E9"
      }
    },
    "Plugins": {
      "Button": {
        "installBackground": "#144d69",
        "updateBackground": "#144d69",
        "installForeground": "#ffffff",
        "installFillForeground": "#ffffff",
        "updateForeground": "#ffffff"
      }
    },
    "EditorTabs": {
      "background": "#F1F0E9",
      "underTabsBorderColor": "#F1F0E9",

      "underlinedTabBackground": "#D3D3D3",
      "underlinedBorderColor": "#D3D3D3",

      "inactiveUnderlinedTabBackground": "#D3D3D3",
      "inactiveUnderlinedTabBorderColor": "#D3D3D3",
      "hoverBackground": "#dadadb",

      "tabInsets": "-6,8,-6,8",
      "tabInsets.compact": "-2,6,-2,4",
      "tabContentActionsRightInsets": "0,2,0,2"
    },
    "Popup": {
      "Header.activeBackground": "#F1F0E9"
    },
    "TabbedPane": {
      "hoverColor": "#F1F0E9"
    },
    "Button": {
      "focus": "#F1F0E9"
    },
    "ActionButton": {
      "hoverBackground": "#F1F0E9",
      "hoverBorderColor": "#F1F0E9",
      "pressedBackground": "#dadadb",
      "pressedBorderColor": "#dadadb"
    }
  }
}