{
  "name": "Blue Dolphin",
  "author": "Shubham",
  "dark": true,
  "editorScheme": "/themes/blueDolphin.xml",
  "colors": {
    "background": "#0f445b",
    "mainBackground": "#215670",
    "treeSelection": "#2d617c",
    "popupBackground": "#1b506a",
    "tabBackground": "#2d617c",
    "hover": "#1C4F67"
  },
  "ui": {
    "*": {
      "background": "background",
      "foreground": "#b0bec5ff",
      "highlight": "#425b67ff",
      "selectionBackground": "treeSelection",
      "selectionInactiveBackground": "treeSelection",
      "selectionForeground": "#ffffffff",
      "button": "#2e3c43ff",
      "secondaryBackground": "#32424aff",
      "disabled": "#415967ff",
      "contrast": "#1e272cff",
      "tableSelected": "#314549ff",
      "secondBorder": "#2a373eff",
      "treeSelection": "treeSelection",
      "notifications": "#323232ff",
      "accent": "#009688ff",
      "excluded": "#004e72ff"
    },
    "Islands": 1,
    "Island": {
      "arc": 20,
      "borderWidth": 6,
      "borderColor": "background",
      "inactiveAlpha": 0.44,
      "inactiveAlphaInStatusBar": {
        "os.mac": 0.2,
        "os.windows": 0.2,
        "os.linux": 0.15
      }
    },
    "MainWindow.background": "mainBackground",
    "DefaultTabs": {
      "background": "background",
      "borderColor": "background"
    },
    "EditorTabs": {
      "background": "background",
      "underTabsBorderColor": "background",

      "underlinedTabBackground": "tabBackground",
      "underlinedBorderColor": "tabBackground",

      "inactiveUnderlinedTabBackground": "tabBackground",
      "inactiveUnderlinedTabBorderColor": "tabBackground",

      "hoverBackground": "hover",

      "tabInsets": "-6,8,-6,8",
      "tabInsets.compact": "-2,6,-2,4",
      "tabContentActionsRightInsets": "0,2,0,2"
    },
    "Popup": {
      "Header.activeBackground": "popupBackground",
      "background": "popupBackground",
      "borderColor": "popupBackground"
    },
    "List": {
      "background": "selectionBackground",
      "mainBackground": "treeSelection"
    },
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "background",
        "background": "background"
      },
      "background": "background"
    },
    "Plugins": {
      "Button": {
        "installBackground": "#005983ff",
        "updateBackground": "#005983ff",
        "installForeground": "#ffffff",
        "installFillForeground": "#ffffff",
        "updateForeground": "#ffffff"
      }
    },
    "TabbedPane": {
      "hoverColor": "#002b3f"
    },
    "Button": {
      "focus": "#002b3f"
    },
    "ActionButton": {
      "hoverBackground": "hover",
      "hoverBorderColor": "background",
      "pressedBackground": "background",
      "pressedBorderColor": "background"
    },
    "ToolTip": {
      "background": "popupBackground",
      "borderColor": "popupBackground"
    },
    "Notification": {
      "background": "popupBackground",
      "borderColor": "popupBackground"
    },
    "EditorNotificationBar": {
      "background": "popupBackground",
      "borderColor": "popupBackground"
    },
    "SuggestedPluginNotification": {
      "background": "popupBackground",
      "borderColor": "popupBackground"
    }
  }
}