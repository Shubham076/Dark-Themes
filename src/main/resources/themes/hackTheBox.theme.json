{
  "name": "Hack The Box",
  "author": "Shubham",
  "dark": true,
  "editorScheme": "/themes/hackTheBox.xml",
  "colors": {
    "background": "#2B384DFF",
    "foreground": "#dadbc0",
    "selectionBackground": "#455065",
    "treeSelection": "#3a485e",
    "hover": "#344052"
  },
  "ui": {
    "*": {
      "background": "background",
      "foreground": "foreground",
      "highlight": "#425b67ff",
      "selectionBackground": "treeSelection",
      "selectionInactiveBackground": "treeSelection",
      "selectionForeground": "foreground",
      "border": "404d64",
      "button": "#2e3c43ff",
      "secondaryBackground": "#32424aff",
      "disabled": "#adae94",
      "contrast": "#1e272cff",
      "tableSelected": "#303d53",
      "secondBorder": "#303d53",
      "treeSelection": "#303d53",
      "notifications": "#323232ff",
      "accent": "#009688ff",
      "excluded": "#263348ff"
    },
    "Islands": 1,
    "Island": {
      "arc": 20,
      "borderWidth": 4,
      "borderColor": "background",
      "inactiveAlpha": 0.44,
      "inactiveAlphaInStatusBar": {
        "os.mac": 0.2,
        "os.windows": 0.2,
        "os.linux": 0.15
      }
    },
    "MainWindow.background": "selectionBackground",
    "DefaultTabs": {
      "background": "background",
      "borderColor": "background"
    },
    "EditorTabs": {
      "background": "background",
      "underTabsBorderColor": "background",

      "underlinedTabBackground": "selectionBackground",
      "underlinedBorderColor": "selectionBackground",

      "inactiveUnderlinedTabBackground": "selectionBackground",
      "inactiveUnderlinedTabBorderColor": "selectionBackground",

      "hoverBackground": "hover",

      "tabInsets": "-6,8,-6,8",
      "tabInsets.compact": "-2,6,-2,4",
      "tabContentActionsRightInsets": "0,2,0,2"
    },
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "background",
        "background": "background"
      },
      "background": "background"
    },
    "Plugins": {
      "Button": {
        "installBackground": "#2b3a54ff",
        "updateBackground": "#2b3a54ff",
        "installForeground": "#ffffff",
        "installFillForeground": "#ffffff",
        "updateForeground": "#ffffff"
      }
    },
    "Popup": {
      "Header.activeBackground": "background"
    },
    "TabbedPane": {
      "hoverColor": "#202b3d"
    },
    "Button": {
      "focus": "#202b3d"
    },
    "ActionButton": {
      "hoverBackground": "hover",
      "hoverBorderColor": "background",
      "pressedBackground": "background",
      "pressedBorderColor": "background"
    }
