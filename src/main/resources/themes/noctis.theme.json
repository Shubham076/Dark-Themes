{
  "name": "Noctis",
  "author": "Shubham",
  "dark": true,
  "editorScheme": "/themes/noctis.xml",
  "colors": {
    "background": "#102f33",
    "selectionBackground": "#09454D",
    "hover": "#073B42"
  },
  "ui": {
    "*": {
      "background": "background",
      "foreground": "#b0bec5ff",
      "highlight": "#425b67ff",
      "selectionBackground": "#08383eff",
      "selectionInactiveBackground": "#08383eff",
      "selectionForeground": "#ffffffff",
      "button": "#2e3c43ff",
      "secondaryBackground": "#32424aff",
      "disabled": "#415967ff",
      "contrast": "#1e272cff",
      "tableSelected": "#314549ff",
      "secondBorder": "#2a373eff",
      "treeSelection": "#00485c",
      "notifications": "#323232ff",
      "accent": "#009688ff",
      "excluded": "#08383eff"
    },
    "Islands": 1,
    "Island": {
      "arc": 20,
      "borderWidth": 4,
      "borderColor": "background",
      "inactiveAlpha": 0.44,
      "inactiveAlphaInStatusBar": {
        "os.mac": 0.2,
        "os.windows": 0.2,
        "os.linux": 0.15
      }
    },
    "MainWindow.background": "selectionBackground",
    "DefaultTabs": {
      "background": "background",
      "borderColor": "background"
    },
    "EditorTabs": {
      "background": "background",
      "underTabsBorderColor": "background",

      "underlinedTabBackground": "selectionBackground",
      "underlinedBorderColor": "selectionBackground",

      "inactiveUnderlinedTabBackground": "selectionBackground",
      "inactiveUnderlinedTabBorderColor": "selectionBackground",

      "hoverBackground": "hover",

      "tabInsets": "-6,8,-6,8",
      "tabInsets.compact": "-2,6,-2,4",
      "tabContentActionsRightInsets": "0,2,0,2"
