{
  "name": "Brackets Light",
  "author": "Shubham Dogra",
  "dark": false,
  "editorScheme": "/themes/bracketsLight.xml",
  "colors": {
    "borderColor": "#c5d1d7",
    "separatorColor": "#c5d1d7",
    "editorBackground": "#fdfeff",
    "menuBackground": "#e4ecef",
    "sidePanelBackground": "#d0e0e8",
    "headerBackground": "#c7dae1",
    "inputBackground": "#fdfeff",
    "tabHoverBackground": "#f6d4c8",
    "hoverBackground": "#d5d8e7",
    "selectionBackground": "#D4DDE7",
    "selectionInactiveBackground": "#c3dbe5",
    "lineColor": "#ED7E5A",
    "alternateBackground": "#ED7E5A",
    "tabColor": "#fdfeff",
    "searchMatchBackground": "#f6d4c8",
    "accentColor": "#1a536e",
    "primaryForeground": "#0e3448",
    "secondaryForeground": "#567b8a",
    "error": "#CE5E5E",
    "errorFocused": "#cb4848",
    "warning": "#CC9D57",
    "warningFocused": "#d09136",
    "info": "#397fb7",
    "success": "#30a25e",
    "background": "#FEFEFE",
    "hover": "#E6EDF5"
  },
  "ui": {
    "*": {
      "arc": "7",
      "foreground": "primaryForeground",
      "background": "background",
      "selectionForeground": "primaryForeground",
      "selectionInactiveForeground": "primaryForeground",
      "selectionBackground": "selectionBackground",
      "selectionInactiveBackground": "selectionInactiveBackground",
      "inactiveBackground": "menuBackground",
      "disabledBackground": "menuBackground",
      "borderColor": "borderColor",
      "separatorColor": "separatorColor",
      "iconColor": "accentColor"
    },
    "Islands": 1,
    "Island": {
      "arc": 20,
      "borderWidth": 4,
      "borderColor": "background",
      "inactiveAlpha": 0.44,
      "inactiveAlphaInStatusBar": {
        "os.mac": 0.2,
        "os.windows": 0.2,
        "os.linux": 0.15
      }
    },
    "MainWindow.background": "selectionBackground",
    "Borders": {
      "color": "borderColor",
      "ContrastBorderColor": "borderColor"
    },
    "Button": {
      "foreground": "primaryForeground",
      "startBorderColor": "borderColor",
      "endBorderColor": "borderColor",
      "startBackground": "inputBackground",
      "endBackground": "inputBackground",
      "focusedBorderColor": "lineColor",
      "disabledBorderColor": "borderColor",
      "default": {
        "foreground": "primaryForeground",
        "startBackground": "inputBackground",
        "endBackground": "inputBackground",
        "startBorderColor": "borderColor",
        "endBorderColor": "borderColor",
        "focusColor": "lineColor",
        "focusedBorderColor": "lineColor"
      }
    },
    "ComboBox": {
      "modifiedItemForeground": "accentColor",
      "ArrowButton": {
        "background": "inputBackground",
        "nonEditableBackground": "inputBackground",
        "disabledIconColor": "selectionInactiveBackground",
        "iconColor": "lineColor"
      },
      "selectionBackground": "selectionBackground",
      "nonEditableBackground": "inputBackground"
    },
    "CompletionPopup": {
      "selectionBackground": "selectionBackground",
      "selectionInactiveBackground": "selectionInactiveBackground",
      "matchForeground": "accentColor"
    },
    "Component": {
      "focusColor": "lineColor",
      "borderColor": "borderColor",
      "focusedBorderColor": "selectionBackground",
      "disabledBorderColor": "selectionInactiveBackground",
      "infoForeground": "secondaryForeground",
      "errorFocusColor": "errorFocused",
      "inactiveErrorFocusColor": "error",
      "warningFocusColor": "warningFocused",
      "inactiveWarningFocusColor": "warning"
    },
    "DragAndDrop": {
      "borderColor": "selectionBackground"
    },
    "Editor": {
      "background": "menuBackground",
      "shortcutForeground": "accentColor"
    },
    "EditorTabs": {
      "background": "background",
      "underTabsBorderColor": "background",

      "underlinedTabBackground": "selectionBackground",
      "underlinedBorderColor": "selectionBackground",

      "inactiveUnderlinedTabBackground": "selectionBackground",
      "inactiveUnderlinedTabBorderColor": "selectionBackground",

      "hoverBackground": "hover",

      "tabInsets": "-6,8,-6,8",
      "tabInsets.compact": "-2,6,-2,4",
      "tabContentActionsRightInsets": "0,2,0,2"
    },
    "Label": {
      "errorForeground": "error",
      "infoForeground": "secondaryForeground"
    },
    "Link": {
      "activeForeground": "accentColor",
      "hoverForeground": "accentColor",
      "visitedForeground": "info",
      "pressedForeground": "info"
    },
    "Notification": {
      "borderColor": "borderColor",
      "errorBorderColor": "error",
      "errorBackground": "menuBackground",
      "errorForeground": "primaryForeground",
      "ToolWindow": {
        "warningForeground": "primaryForeground",
        "warningBackground": "menuBackground",
        "warningBorderColor": "warning",
        "errorForeground": "primaryForeground",
        "errorBackground": "menuBackground",
        "errorBorderColor": "error",
        "informativeForeground": "primaryForeground",
        "informativeBackground": "menuBackground",
        "informativeBorderColor": "info"
      }
    },
    "NotificationsToolwindow": {
      "newNotification": {
        "hoverBackground": "hoverBackground",
        "background": "menuBackground"
      },
      "Notification": {
        "hoverBackground": "hoverBackground"
      }
    },
    "PasswordField": {
      "background": "inputBackground"
    },
    "Plugins": {
      "SearchField": {
        "background": "editorBackground"
      },
      "SectionHeader": {
        "foreground": "primaryForeground"
      },

      "hoverBackground": "hoverBackground",
      "lightSelectionBackground": "selectionBackground",
      "Button": {
        "installBorderColor": "accentColor",
        "installForeground": "accentColor",
        "installBackground": "menuBackground",
        "installFillForeground": "accentColor",
        "installFillBackground": "menuBackground",
        "installFocusedBackground": "selectionBackground",
        "updateBorderColor": "accentColor",
        "updateForeground": "accentColor",
        "updateBackground": "menuBackground"
      },
      "Tab": {
        "selectedBackground": "hoverBackground",
        "selectedForeground": "primaryForeground",
        "hoverBackground": "hoverBackground"
      }
    },
    "ProgressBar": {
      "trackColor": "inputBackground",
      "indeterminateStartColor": "inputBackground",
      "indeterminateEndColor": "inputBackground",
      "progressColor": "lineColor",
      "passedColor": "lineColor",
      "passedEndColor": "lineColor",
      "failedColor": "error",
      "failedEndColor": "error"
    },
    "Popup": {
      "Header": {
        "activeBackground": "headerBackground",
        "inactiveBackground": "headerBackground"
      }
    },
    "ScrollBar": {
      "Mac": {
        "hoverThumbColor": "selectionBackground",
        "Transparent": {
          "hoverThumbColor": "selectionInactiveBackground"
        }
      }
    },
    "SearchEverywhere": {
      "SearchField": {
        "background": "editorBackground"
      },
      "Tab": {
        "selectedBackground": "editorBackground",
        "selectedForeground": "primaryForeground"
      }
    },
    "SearchMatch": {
      "startBackground": "searchMatchBackground",
      "endBackground": "searchMatchBackground"
    },
    "Separator": {
      "separatorColor": "separatorColor"
    },
    "SidePanel": {
      "background": "sidePanelBackground"
    },
    "TabbedPane": {
      "tabSelectionHeight": 1,
      "focusColor": "hoverBackground",
      "hoverColor": "hoverBackground",
      "underlineColor": "lineColor",
      "contentAreaColor": "hoverBackground"
    },
    "Table": {
      "gridColor": "borderColor",

      "hoverBackground": "hoverBackground",
      "lightSelectionBackground": "selectionBackground"
    },
    "TableHeader": {
      "bottomSeparatorColor": "separatorColor"
    },
    "TextField": {
      "background": "editorBackground"
    },
    "ToggleButton": {
      "onBackground": "lineColor",
      "onForeground": "hoverBackground",
      "offBackground": "selectionBackground",
      "offForeground": "hoverBackground",
      "buttonColor": "primaryForeground"
    },
    "ToolWindow": {
      "Button": {
        "selectedBackground": "alternateBackground",
        "hoverBackground": "hoverBackground"
      },
      "Header": {
        "background": "background",
        "inactiveBackground": "background"
      },
      "HeaderTab": {
        "underlineColor": "lineColor",
        "underlineHeight": 3,
        "underlinedTabBackground": "tabColor",
        "underlinedTabInactiveBackground": "tabColor"
      },
      "background": "background"
    },
    "Tree": {
      "modifiedItemForeground": "accentColor",
      "selectionBackground": "selectionBackground",
      "selectionInactiveBackground": "selectionInactiveBackground",
      "hash": "hoverBackground"
    },
    "ValidationTooltip": {
      "errorBackground": "error",
      "warningBackground": "warning"
    },
    "VersionControl": {
      "FileHistory": {
        "Commit": {
          "selectedBranchBackground": "selectionBackground"
        }
      },
      "GitLog": {
        "headIconColor": "warning",
        "localBranchIconColor": "accentColor",
        "remoteBranchIconColor": "info",
        "tagIconColor": "lineColor",
        "otherIconColor": "error"
      },
      "Log": {
        "Commit": {
          "hoveredBackground": "hoverBackground",
          "currentBranchBackground": "selectionBackground"
        }
      },
      "RefLabel": {
        "foreground": "primaryForeground"
      }
    },
    "Menu": {
      "background": "menuBackground"
    },
    "WelcomeScreen": {
      "SidePanel": {
        "background": "sidePanelBackground"
      },
      "separatorColor": "separatorColor",
      "Projects": {
        "background": "menuBackground",
        "selectionBackground": "selectionBackground",
        "selectionInactiveBackground": "selectionInactiveBackground",
        "actions": {
          "background": "menuBackground"
        }
      }
    },
    "DefaultTabs": {
      "background": "background",
      "borderColor": "background"
    },
    "ActionButton": {
      "hoverBackground": "hover",
      "hoverBorderColor": "background",
      "pressedBackground": "background",
      "pressedBorderColor": "background"
    }
  },
  "icons": {
    "ColorPalette": {
      "Tree.iconColor": "#567b8a",
      "Actions.Blue": "#5da3f4",
      "Actions.Green": "#2fc864",
      "Actions.Grey": "#858994",
      "Actions.GreyInline.Dark": "#2fc864",
      "Actions.GreyInline": "#2fc864",
      "Actions.Red": "#CE5E5E",
      "Actions.Yellow": "#f1fa8c",
      "Objects.Blue": "#5da3f4",
      "Objects.Green": "#2fc864",
      "Objects.GreenAndroid": "#2fc864",
      "Objects.Grey": "#858994",
      "Objects.Pink": "#ff79c6",
      "Objects.Purple": "#bd93f9",
      "Objects.Red": "#CE5E5E",
      "Objects.RedStatus": "#CE5E5E",
      "Objects.Yellow": "#f1fa8c",
      "Objects.YellowDark": "#f1fa8c",
      "Objects.BlackText": "#112733"
    }
  }
}