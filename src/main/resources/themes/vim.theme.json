{
  "author": "Shubham",
  "dark": true,
  "name": "Vim Dark",
  "editorScheme": "/themes/vim.xml",
  "ui": {
    "*": {
      "background": "#2d2d2d",
      "foreground": "#ebdbb2",
      "highlight": "#425b67ff",
      "selectionBackground": "#424242",
      "selectionInactiveBackground": "#424242",
      "selectionForeground": "#ffffffff",
      "button": "#2e3c43ff",
      "secondaryBackground": "#32424aff",
      "disabled": "#415967ff",
      "contrast": "#1e272cff",
      "tableSelected": "#314549ff",
      "secondBorder": "#2a373eff",
      "treeSelection": "#00485c",
      "notifications": "#323232ff",
      "accent": "#009688ff",
      "excluded": "#424242"
    },
    "DefaultTabs": {
      "background": "#2d2d2d",
      "borderColor": "#1f2430"
    },
    "EditorTabs": {
      "underlinedTabBackground": "#2d2d2d"
    },
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "#2d2d2d"
      }
    },
     "Plugins": {
      "Button": {
        "installBackground": "#424242",
        "updateBackground": "#424242",
        "installForeground": "#ffffff",
        "installFillForeground": "#ffffff",
        "updateForeground": "#ffffff"
      }
    },
    "Popup": {
      "Header.activeBackground": "#2d2d2d"
    },
    "TabbedPane": {
      "hoverColor": "#2d2d2d"
    },
    "Button": {
      "focus": "#2d2d2d"
    }
  }
}