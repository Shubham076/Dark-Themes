{
  "name": "Synthwave Refined",
  "author": "Shubham",
  "dark": true,
  "editorScheme": "/themes/synthwaveRefined.xml",
  "colors": {
    "background": "#302D3F",
    "selectionBackground": "#413E4E",
    "tabBackground": "#4c4959",
    "hover": "#3F3C4F"
  },
  "ui": {
    "*": {
      "background": "background",
      "foreground": "#b0bec5ff",
      "highlight": "#425b67ff",
      "selectionBackground": "#352745",
      "selectionInactiveBackground": "#352745",
      "selectionForeground": "#ffffffff",
      "button": "#2e3c43ff",
      "secondaryBackground": "#32424aff",
      "disabled": "#415967ff",
      "contrast": "#1e272cff",
      "tableSelected": "#314549ff",
      "secondBorder": "#2a373eff",
      "treeSelection": "#00485c",
      "notifications": "#323232ff",
      "accent": "#009688ff",
      "excluded": "#352745"
    },
    "Islands": 1,
    "Island": {
      "arc": 20,
      "borderWidth": 7,
      "borderColor": "background",
      "inactiveAlpha": 0.44,
      "inactiveAlphaInStatusBar": {
        "os.mac": 0.2,
        "os.windows": 0.2,
        "os.linux": 0.15
      }
    },
    "MainWindow.background": "selectionBackground",
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "background",
        "background": "background"
